 {
  "manifest_version": 2,
  "name": "Nutte Gesicht",
  "version": "0.1",
  
  "icons": {
    "128": "icon.png"
  },
  "content_scripts": [
    {
      "matches": [ "*://*/*" ],
      "js": [ "popup.js"]
    }
  ],
  "permissions" : [ "tabs", "http://*/*", "https://*/*" ],

      "browser_action": {
        "default_title": "Nutte Gesicht",
        "default_icon": "icon.png",
        "default_popup": "popup.html"
    }
	
}